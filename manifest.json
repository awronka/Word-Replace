{
	"manifest_version": 2,
	"name": "Word Replace",
	"version": "1.0",
	"description": "Replaces words on the dom",
	"options_page": "options/index.html",
    "permissions": [
      "storage"
    ],
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["actions/content_script.js","server/save_script.js"],
			"run_at": "document_end"
		}
	]
}